{
  "name": "skynettechnologies/bolt-allinoneaccessibility",
  "description": "Quick Web Accessibility Implementation with All In One Accessibility!",
  "version": "1.5",
  "type": "bolt-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Rajesh Bhimani",
      "email": "developer3@skynettechnologies.com"
    }
  ],
  "keywords": [
    "bolt",
    "cms",
    "symphony",
    "accessibility",
    "wcag",
    "ada",
    "All",
    "in",
    "One",
    "Accessibility"
  ],
  "require": {
    "php": ">=7.2.9"
  },
  "require-dev": {
    "bolt/core": "^4.0 || ^5.0",
    "symplify/easy-coding-standard": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Skynettechnologies\\BoltAllinoneaccessibility\\": "src/"
    }
